{
  "Vendor": true,
  "DisableAll": true,
  "Enable": [
    "vet",
    "safesql",
    "errcheck",
    "goconst",
    "goimports",
    "varcheck",
    "gosec",
    "staticcheck",
    "gosimple",
    "lll",
    "unconvert",
    "misspell",
    "unconvert"
